<?xml version="1.0"?>
<launch>
  <!-- load the arm controller -->
  <rosparam file="$(find manipulator_description)/config/main_controller.yaml" command="load" />

  <!-- Load the controllers -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
    output="screen" args="arm_controller"/>

</launch>